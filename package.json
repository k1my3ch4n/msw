{
  "name": "msw",
  "packageManager": "yarn@4.3.0",
  "scripts": {
    "jest": "yarn workspace jest",
    "vitest": "yarn workspace vitest"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "references": [
    {
      "path": "packages/jest"
    },
    {
      "path": "packages/vitest"
    }
  ],
  "exclude": [
    "packages/**/dist/**"
  ],
  "include": []
}
